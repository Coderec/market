<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
	
<struts>
	<package name="adminDetail" namespace="/adminDetail" extends="struts-default">
		<interceptors>
        <interceptor-stack name="secureDefaultStack">
            <interceptor-ref name="defaultStack">
                <param name="params.excludeParams">(.*\.|^|.*|\[('|"))(c|C)lass(\.|('|")]|\[).*,^dojo\..*,^struts\..*,^session\..*,^request\..*,^application\..*,^servlet(Request|Response)\..*,^parameters\..*,^action:.*,^method:.*</param>
            </interceptor-ref>
        </interceptor-stack>
    	</interceptors>

    	<default-interceptor-ref name="secureDefaultStack" />
		
  		<action name="adminDetailAction_*" method="{1}" class="adminDetailAction">
  		<result name="success">/AdminDetail.jsp</result>
  		</action>
	</package>
</struts>	